{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "consistent-return": 0,
    "strict": 0,
    "quotes": 0,
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "new-cap": 0,
    "comma-spacing": 0,
    "no-use-before-define": 0,
    "curly": 0,
    "no-trailing-spaces": 0,
    "semi-spacing": 0,
    "react/jsx-uses-react": [2],
    "no-unused-expressions": 0,
    "dot-notation": [2, {"allowPattern": "^NODE_ENV$"}],
    "comma-dangle": 0,
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "window": true,
    "document": true
  }
}
